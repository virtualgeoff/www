#VRML V2.0 utf8

WorldInfo {
   title		"Basic world - Geometry primitives"
   info		"Geoff Pack, 1997"
}

NavigationInfo {
   headlight 	TRUE
   type		"EXAMINE"	#WALK, FLY, EXAMINE, NONE
   avatarSize  	1.8
   speed		1.0
}

Viewpoint {
   description "Entry"
   position 0 0 10
}

Transform {
   translation 		0 0 0
   rotation			0 0 0 0
   scale			1 1 1
   center			0 0 0
   children [
	Shape {
 	   appearance Appearance {
		material Material {
		   diffuseColor	0 .9 0	
		   shininess	.5
		}
	   }
	   geometry Sphere {
		radius	1
	   }
	}
   ]
}

Transform {
   translation 		3 0 0
   rotation			0 0 0 0
   scale			1 1 1
   center			0 0 0
   children [
	Shape {
 	   appearance Appearance {
		material Material {
		   diffuseColor	0 0 1
		   shininess	.5
		}
	   }
	   geometry Cylinder {
		radius	1
		height	2
		side		TRUE
		top		TRUE
		bottom	TRUE
	   }
	}
   ]
}

Transform {
   translation 		6 0 0
   rotation			0 0 0 0
   scale			1 1 1
   center			0 0 0
   children [
	Shape {
 	   appearance Appearance {
		material Material {
		   diffuseColor	1 1 0
		   shininess	.5
		}
	   }
	   geometry Cone {
		bottomRadius	1
		height		2
		side			TRUE
		bottom		TRUE
	   }
	}
   ]
}


Transform {
   translation 		-3 0 0
   rotation			0 0 0 0
   scale			1 1 1
   center			0 0 0
   children [
	Shape {
 	   appearance Appearance {
		material Material {
		   diffuseColor	1 0 0
		   shininess	.5
		}
	   }
	   geometry Box {
		size		2 2 2
	   }
	}
   ]
}
