#VRML V2.0 utf8# Inline and Frames Demo# Geoff Pack, 1998DEF HOME Viewpoint {   description 	"Home"   position 	0 0 10}DEF GREEN Viewpoint {   description 	"Green"   position 	0 0 5}DEF RED Viewpoint {   description 	"Red"   position 	-15 0 5   orientation	0 1 0 -1.57} DEF BLUE Viewpoint {   description 	"Blue"   position 	15 0 5   orientation	0 1 0 1.57}DEF GREEN Viewpoint {   description 	"Green"   position 	0 0 10}Transform {	translation -5 0 0	children [		Anchor {			url			["red.html"]			parameter 	["target=right"]			children [				Inline {					url	["red.wrl"]					bboxCenter	0 0 0					bboxSize	4 4 4				}			]		}	]}Transform {	translation 0 0 0	children [		Anchor {			url	["green.html"]			parameter 	["target=right"]			children [				Inline {					url	["green.wrl"]					bboxCenter	0 0 0					bboxSize	4 4 4				}			]		}	]			}Transform {	translation 5 0 0	children [		Anchor {			url	["blue.html"]			parameter 	["target=right"]			children [				Inline {					url	["blue.wrl"]					bboxCenter	0 0 0					bboxSize	4 4 4				}			]		}	]}